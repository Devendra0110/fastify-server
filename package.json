    "scripts": {
        "start": "tsc --project tsconfig.json && node dist",
        "start:dev": "tsnd --inspect --respawn  src/server.ts",
        "test:prod": "ts-node node_modules/jasmine/bin/jasmine --config=spec/support/jasmine.json",
        "test:dev": "ts-node node_modules/jasmine/bin/jasmine --config=spec/support/jasmine.acceptance.json",
        "build:dev": "NODE_ENV='development' tsc",
        "build:prod": "NODE_ENV='production' tsc",
        "preversion": "npm run build:prod",
        "version": "git commit -a -m 'Updated version.' && git push && git checkout master",
        "postversion": "git merge development && git push && git push --tags && git checkout development",
        "test1": "rimraf node_modules/p3l_core/src/*",
        "pm2": "pm2 start ecosystem.config.js --env production"
    },
